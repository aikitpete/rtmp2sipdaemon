#include "crtmpexception.h"

using namespace std;

CRtmpException::CRtmpException()
{
    m_strMessage = string("Empty RTMP Exception");
}

CRtmpException::~CRtmpException()
{

}

CRtmpException::CRtmpException(string strMessage)
{
    m_strMessage = strMessage;
}

std::string CRtmpException::GetMessage() const
{
    return m_strMessage;
}
